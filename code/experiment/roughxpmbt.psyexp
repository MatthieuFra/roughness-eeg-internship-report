        <Param val="console.log(&quot;trial number: &quot;, trials.thisN);&amp;#10;console.log(&quot;sound1: &quot;, sound_file);&amp;#10;oddball = oddballs.pop();&amp;#10;console.log(&quot;is it an oddball ? &quot;, oddball);&amp;#10;if ((oddball === true)) {&amp;#10;    oddball_sound_file = sample(trials.trialList, 1)[0][&quot;sound_file&quot;];&amp;#10;    while ((oddball_sound_file === sound_file)) {&amp;#10;        oddball_sound_file = sample(trials.trialList, 1)[0][&quot;sound_file&quot;];&amp;#10;    }&amp;#10;} else {&amp;#10;    oddball_sound_file = sound_file;&amp;#10;}&amp;#10;console.log(&quot;sound2: &quot;, oddball_sound_file);&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
