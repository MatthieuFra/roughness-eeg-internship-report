        <Param val="# trigger managment&amp;#10;if sound1.status == NOT_STARTED and tThisFlip &gt;= 1-frameTolerance:&amp;#10;    win.callOnFlip(send_triggers, [int(trigger1)])&amp;#10;&amp;#10;# trigger managment&amp;#10;if sound2.status == NOT_STARTED and tThisFlip &gt;= 3-frameTolerance:&amp;#10;    win.callOnFlip(send_triggers, [int(trigger2)])" valType="extendedCode" updates="constant" name="Each Frame"/>
