        <Param val="def send_triggers(value):&amp;#10;    ser.write(value)&amp;#10;    outlet.push_sample(value)&amp;#10;&amp;#10;ser = serial.Serial()&amp;#10;ser.port = expInfo['serial port']&amp;#10;ser.open()&amp;#10;&amp;#10;# Creating the LSL stream:&amp;#10;info = StreamInfo(name='lsltriggerstream', type='Markers', channel_count=1, channel_format='int32') #Initialize LSL stream for markers&amp;#10;outlet = StreamOutlet(info)  # Broadcast the stream.&amp;#10;&amp;#10;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
