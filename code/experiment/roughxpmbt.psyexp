        <Param val="# trigger managment&amp;#10;trigger1 = str(sound_file)[11:13]&amp;#10;print(&quot;trigger1: &quot;, trigger1)&amp;#10;&amp;#10;trigger2 = str(oddball_sound_file)[11:13]&amp;#10;print(&quot;trigger2: &quot;, trigger2)" valType="extendedCode" updates="constant" name="Begin Routine"/>
